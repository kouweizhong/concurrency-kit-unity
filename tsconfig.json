{
  "compilerOptions": {
    "declaration": true,
    "module": "umd",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./",
    "preserveConstEnums": true,
    "removeComments": false,
    "rootDir": "./",
    "sourceMap": true,
    "target": "es6"
  },
  "exclude": [
    "node_modules",
    "Artifacts",
    "Assets",
    "Library",
    "obj",
    "ProjectSettings",
    "gulpfile.ts"
  ]
}